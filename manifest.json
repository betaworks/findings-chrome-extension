{
  "name": "Findings",
  "version": "0.9",
  "manifest version": 2,
  "description": "The Findings Chrome extension",
  "background": {
  	"page": "fdgs_background.html"
  },
  "browser_action": {
  	"default_icon": "icon.png",
  	"default_title": "Findings.com"
  },
  "options_page": "options.html",
  "permissions": [
  	"tabs",
    "notifications",
  	"http://*/*",
  	"https://*/*"
  ]
}
